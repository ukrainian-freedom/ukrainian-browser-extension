{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["index.js", "scripts/{build,create}.ts", "src/index.ts"],
  "ignore": ["src/env.ts"],
  "ignoreBinaries": ["cross-env", "eslint"],
  "ignoreDependencies": [
    "@biomejs/biome",
    "@eslint/eslintrc",
    "@eslint/js",
    "@stylistic/eslint-plugin",
    "@swc/cli",
    "@swc/core",
    "@types/eslint__js",
    "dotenv",
    "eslint-plugin-eslint-comments",
    "eslint-plugin-import-x",
    "eslint-plugin-json",
    "eslint-plugin-no-relative-import-paths",
    "eslint-plugin-perfectionist",
    "eslint-plugin-promise",
    "eslint-plugin-sonarjs",
    "eslint-plugin-unicorn",
    "globals",
    "typescript-eslint"
  ],
  "project": ["index.js", "scripts/**/*.ts", "src/**/*.ts"]
}
